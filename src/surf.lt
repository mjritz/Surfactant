                                                                                                 
import "miesurf_ff.lt"                                                                                   

Surf {                                                                                                   
                                                                                                             
  write('Data Atoms') {                                                                                  
    $atom:CM1 $mol:. @atom:MieSurf/CM  0.00    0.0000    0.0000    0.0000
    $atom:CM2 $mol:. @atom:MieSurf/CM  0.00    0.0000    0.0000    4.5000
    $atom:CM3 $mol:. @atom:MieSurf/CM  0.00    0.0000    0.0000    9.0000
    $atom:CM4 $mol:. @atom:MieSurf/CM  0.00    0.0000    0.0000   13.5000
    $atom:OA1 $mol:. @atom:MieSurf/OA  0.00    0.0000    0.0000  18.0000
    $atom:OA2 $mol:. @atom:MieSurf/OA  0.00    0.0000    0.0000  22.5000
    $atom:CM5 $mol:. @atom:MieSurf/CM  0.00    0.0000   -9.0000    4.5000
    $atom:CM6 $mol:. @atom:MieSurf/CM  0.00    0.0000   -4.5000    4.5000
